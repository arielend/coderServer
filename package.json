{
  "name": "coderserver",
  "version": "1.0.0",
  "description": "Servidor creado en el curso backend de Coderhouse, por Natalia Peripolli y Ariel Endrizzi",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node ./server.js --port 8080 --env production --pers mongo",
    "dev": "nodemon ./server.js --port 8080 --env development --pers mongo",
    "test": "node ./server.js --port 8081 --env testing --pers memory",
    "fs": "node ./server.js --port 8083 --env test --pers fs",
    "mocks": "node ./src/mocks/mocks.js",
    "simplex": "artillery quick --count 20 --num 100 http://localhost:9000/api/test/simplex -o ./test/artillery/simplex.json",
    "complex": "artillery quick --count 20 --num 100 http://localhost:9000/api/test/complex -o ./test/artillery/complex.json",
    "flow": "artillery run ./test/artillery/flow.yaml -o ./test/artillery/flow.json",
    "report": "artillery report ./test/artillery/flow.json"
  },
  "keywords": [],
  "author": "Ariel Endrizzi",
  "license": "ISC",
  "dependencies": {
    "artillery-plugin-metrics-by-endpoint": "^1.10.0",
    "bcrypt": "^5.1.1",
    "commander": "^12.1.0",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-compression": "^1.0.2",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.18.0",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.2",
    "mongoose-paginate-v2": "^1.8.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.7.5",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "nodemon": "^3.1.0"
  }
}
